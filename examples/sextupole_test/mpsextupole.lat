! File sextupole.lat
!
! a single thin multipole with sextupole component for testing

length   :=   2.0                 ! [m]     : sextupole length

strength := 0.211     ! [m**-2] : sextupole strength
                                  !         :   = B''/brho

s_times_l = strength*length

beam, particle=proton, pc=8.9

mysexp: multipole, type=isb, l=0.0, k2l = s_times_l

mylat2: line = (mysexp)
