! File sextupole.lat
!
! a single thin sextupole for testing

length   :=   2.0                 ! [m]     : sextupole length

strength := 0.211     ! [m**-2] : sextupole strength
                                  !         :   = B''/brho

s_times_l = strength*length

beam, particle=proton, pc=8.9

mysex: sextupole, type=isa, l=0.0, k2 = s_times_l

mylat1: line = (mysex)
